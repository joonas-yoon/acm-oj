{
    "name": "cloud5ideas/appkit",
    "description": "Application kit for Laravel 5",
    "keywords": ["laravel", "appkit", "modules", "menus", "javascript", "faker", "flash", "cloud5ideas"],
    "license": "MIT",
    "authors": [
        {
            "name": "Charl Gottschalk",
            "email": "cgottschalk@cloud5.co.za"
        }        
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/database": "~5.0",
        "illuminate/support": "~5.0",
        "illuminate/filesystem": "~5.0",
        "illuminate/config": "~5.0",
        "illuminate/routing": "~5.0",
        "illuminate/html": "~5.0",
        "fzaninotto/faker": "~1.4"
    },
    "autoload": {
        "psr-4": {
            "C5\\AppKit\\": "src/AppKit/"
        },
        "files": [
            "src/AppKit/Functions/functions.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev"
}
